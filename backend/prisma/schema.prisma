generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Registration {
  id            String    @id @default(uuid())
  walletAddress String
  eventName     String
  createdAt     DateTime  @default(now())

  @@unique([walletAddress, eventName])
}
